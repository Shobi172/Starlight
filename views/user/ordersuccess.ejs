<%- include("../partials/userheader.ejs") %>



<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>aranaz</title>
  <link rel="icon" href="img/favicon.png">
  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="../../confirm/css/bootstrap.min.css">
  <!-- animate CSS -->
  <link rel="stylesheet" href="../../confirm/css/animate.css">
  <!-- owl carousel CSS -->
  <link rel="stylesheet" href="../../confirm/css/owl.carousel.min.css">
  <!-- nice select CSS -->
  <link rel="stylesheet" href="../../confirm/css/nice-select.css">
  <!-- font awesome CSS -->
  <link rel="stylesheet" href="../../confirm/css/all.css">
  <!-- flaticon CSS -->
  <link rel="stylesheet" href="../../confirm/css/flaticon.css">
  <link rel="stylesheet" href="../../confirm/css/themify-icons.css">
  <!-- font awesome CSS -->
  <link rel="stylesheet" href="../../confirm/css/magnific-popup.css">
  <!-- swiper CSS -->
  <link rel="stylesheet" href="../../confirm/css/slick.css">
  <link rel="stylesheet" href="../../confirm/css/price_rangs.css">
  <!-- style CSS -->
  <link rel="stylesheet" href="../../confirm/css/style.css">
  </head>



    <section class="confirmation_part padding_top">

    <div class="bg-light py-3">
        <div class="container">
          <div class="row">
            <div class="col-md-12 mb-0"><a href="/">Home</a> <span class="mx-2 mb-0">/</span> <strong class="text-black">Contact</strong></div>
          </div>
        </div>
      </div>  
  
      <div class="site-section">
        <div class="container">
          <div class="row">
            <div class="col-md-12 text-center">
              <span class="icon-check_circle display-3 text-success"></span>
              <h2 class="display-3 text-black">Thank you!</h2>
              <p class="lead mb-5">You order was successfuly completed.</p>

              <p><a href="/shop" class="btn btn-sm btn-primary">Back to shop</a></p>
            </div>
            <div>
              <p><a href="/orderhistory" class="btn btn-sm btn-primary">Order Detials</a></p>
            </div>
          </div>
        </div>
      </div>


    <div class="container">
      <div class="row">
       <div class="col-lg-6 col-lx-4">
          <div class="single_confirmation_details">
            <h4>Billing Address</h4>
            <p>
                <%= deladd.name %><br><%= deladd.address %> <br><%= deladd.state %><br><%= deladd.city %>, <%= deladd.pincode %>
            </p>
          </div>
        </div>
        
        <div class="col-lg-6 col-lx-4">
          <div class="single_confirmation_details">
            <h4>shipping Address</h4>
            <p>
                <%= deladd.name %><br><%= deladd.address %> <br><%= deladd.state %><br><%= deladd.city %>, <%= deladd.pincode %>
            </p>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="order_details_iner">
            <h3>Order Details &emsp; #  <%= id %></h3>
            <table class="table table-borderless">
              <thead>
                <tr>
                  <th scope="col" colspan="2">Product</th>
                  <th scope="col">Quantity</th>
                  <th scope="col">Total</th>
                </tr>
              </thead>
              <tbody>
                
               

                <tr>
                <th colspan="2"><span>Jacket</span></th>
                 <th>x <%= count %></th>
                  <th><span>Rs.<%= amount %></span></th>
                </tr>

             
            
                <tr>
                  <th colspan="3">shipping</th>
                  <th><span>Rs.0.00</span></th>
                </tr>
              </tbody>
              <tfoot>
                <tr>
                  <th scope="col" colspan="3">Total</th>
                  <th scope="col">Rs.<%= amount %></th>
                  
                </tr>
              </tfoot>
            </table>
          </div>
        </div>
      </div>
    </div>

</section>
<br>
<br>
<br>

<%- include("../partials/userfooter.ejs") %>





  
     

      